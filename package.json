{
  "name": "inkdot",
  "version": "1.0.0",
  "description": "InkDot - 创意创作思维导图平台",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "timestamp": "node docs/tools/timestamp-helper.js",
    "timestamp:current": "node docs/tools/timestamp-helper.js current",
    "timestamp:help": "node docs/tools/timestamp-helper.js help",
    "timestamp:scan": "node docs/tools/timestamp-helper.js scan",
    "timestamp:batch": "node docs/tools/timestamp-helper.js batch-update",
    "timestamp:fix": "node docs/tools/timestamp-helper.js fix-headers",
    "timestamp:find": "node docs/tools/timestamp-helper.js find-md",
    "docs:update": "node docs/tools/timestamp-helper.js update-doc",
    "docs:header": "node docs/tools/timestamp-helper.js header",
    "docs:scan": "node docs/tools/timestamp-helper.js scan",
    "docs:batch": "node docs/tools/timestamp-helper.js batch-update",
    "docs:fix": "node docs/tools/timestamp-helper.js fix-headers",
    "changelog": "node docs/tools/changelog-helper.js",
    "changelog:update": "node docs/tools/changelog-helper.js update",
    "changelog:add": "node docs/tools/changelog-helper.js add",
    "changelog:version": "node docs/tools/changelog-helper.js version",
    "changelog:status": "node docs/tools/changelog-helper.js status",
    "changelog:help": "node docs/tools/changelog-helper.js help",
    "change": "node docs/tools/change-entry-helper.js",
    "change:auto": "node docs/tools/change-entry-helper.js auto",
    "change:detect": "node docs/tools/change-entry-helper.js detect",
    "change:add": "node docs/tools/change-entry-helper.js add",
    "change:scan": "node docs/tools/change-entry-helper.js scan",
    "change:interactive": "node docs/tools/change-entry-helper.js interactive",
    "change:help": "node docs/tools/change-entry-helper.js help",
    "status": "node docs/tools/status-update-helper.js",
    "status:update": "node docs/tools/status-update-helper.js update",
    "status:scan": "node docs/tools/status-update-helper.js scan",
    "status:check": "node docs/tools/status-update-helper.js check",
    "status:help": "node docs/tools/status-update-helper.js help",
    "setup": "node scripts/setup-dev-environment.js",
    "setup:dev": "node scripts/setup-dev-environment.js",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:performance": "vitest tests/performance/",
    "test:integration": "vitest tests/integration/",
    "test:unit": "vitest tests/unit/",
    "test:e2e": "playwright test",
    "test:playwright": "node tools/playwright/run-tests.js",
    "test:playwright:all": "node tools/playwright/run-tests.js all",
    "test:playwright:debug": "node tools/playwright/run-tests.js debug",
    "test:playwright:buttons": "node tools/playwright/run-tests.js buttons",
    "test:playwright:chinese": "node tools/playwright/run-tests.js chinese",
    "test:playwright:render": "node tools/playwright/run-tests.js render",
    "test:playwright:fonts": "node tools/playwright/run-tests.js fonts",
    "cleanup": "node tools/cleanup.js",
    "clean": "node tools/cleanup.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install"
  },
  "keywords": [
    "mindmap",
    "ai",
    "creative",
    "writing",
    "vue3",
    "typescript"
  ],
  "author": "InkDot Team",
  "license": "MIT",
  "dependencies": {
    "@vue/runtime-core": "^3.5.21",
    "lucide-vue-next": "^0.544.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.6",
    "playwright": "^1.55.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vite": "^7.0.0",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.md": [
      "node docs/tools/timestamp-helper.js check-timestamp"
    ]
  }
}
